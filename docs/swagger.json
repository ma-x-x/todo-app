    "definitions": {
        "api_v1_handlers.CreateTodoRequest": {
            "type": "object",
            "required": [
                "title"
            ],
            "properties": {
                "category_id": {
                    "description": "CategoryID 所属分类ID，可选",
                    "type": "integer"
                },
                "description": {
                    "description": "Description 待办事项描述，可选，最大长度1024个字符",
                    "type": "string",
                    "maxLength": 1024
                },
                "priority": {
                    "description": "Priority 优先级：1(低)/2(中)/3(高)",
                    "type": "integer",
                    "enum": [
                        1,
                        2,
                        3
                    ]
                },
                "title": {
                    "description": "Title 待办事项标题，必填，最大长度128个字符",
                    "type": "string",
                    "maxLength": 128
